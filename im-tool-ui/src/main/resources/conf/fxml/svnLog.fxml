                                                        <PropertyValueFactory property="version"/>
                                                    </cellValueFactory>
                                                </TableColumn>
                                                <TableColumn id="time" editable="false" prefWidth="182" sortable="false"
                                                             text="提交时间">
                                                    <cellValueFactory>
                                                        <PropertyValueFactory property="time"/>
                                                    </cellValueFactory>
                                                </TableColumn>
                                                <TableColumn id="num" editable="false" prefWidth="150" sortable="false"
                                                             text="文件数量">
                                                    <cellValueFactory>
                                                        <PropertyValueFactory property="num"/>
                                                    </cellValueFactory>
                                                </TableColumn>
                                            </columns>
                                        </TableView>
                                    </children>
                                </AnchorPane>
                            </items>
