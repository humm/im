<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="650.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.hoomoomoo.im.controller.ShoppingStarterController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu fx:id="menuJd" mnemonicParsing="false" text="京东">
                    <items>
                        <MenuItem fx:id="jdAuto" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="自动评价"/>
                        <MenuItem fx:id="waitAppraise" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="待评价"/>
                        <MenuItem fx:id="showOrder" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="待晒单"/>
                        <MenuItem fx:id="appendAppraise" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="待追评"/>
                        <MenuItem fx:id="serviceAppraise" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="服务评价"/>
                    </items>
                </Menu>
                <Menu fx:id="menuSet" mnemonicParsing="false" text="设置">
                    <items>
                        <MenuItem fx:id="configSet" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="参数设置"/>
                    </items>
                    <items>
                        <MenuItem fx:id="jdCookie" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="京东Cookie设置"/>
                    </items>
                </Menu>
                <Menu fx:id="menuHelp" mnemonicParsing="false" text="帮助">
                    <items>
                        <MenuItem fx:id="functionStatInfo" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="使用统计"/>
                        <MenuItem fx:id="aboutInfo" mnemonicParsing="false"
                                  onAction="#openMenu"
                                  text="关于"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <TabPane fx:id="functionTab" layoutX="74.0" layoutY="33.0" prefHeight="375.0" prefWidth="640.0"
                         AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                         AnchorPane.topAnchor="0.0"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
