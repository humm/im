                                        <Label layoutX="14.0" layoutY="55.0" prefHeight="15.0" text="开通脚本目录(最外层目录)"/>
                                        <TextField fx:id="paramRealtimeSetPath" layoutX="200.0" layoutY="51.0"
                                                   prefHeight="23.0" prefWidth="870.0"/>

                                        <Label layoutX="14.0" layoutY="96.0" prefHeight="15.0"
