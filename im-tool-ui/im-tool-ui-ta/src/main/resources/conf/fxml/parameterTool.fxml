                                                mnemonicParsing="false" onAction="#showParamRealtimeSetResult"
                                                prefWidth="80.0" text="查看">
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </padding>
                                        </Button>
                                        <Button fx:id="errorTipsResultByFile" layoutX="1500.0" layoutY="91"
                                                mnemonicParsing="false" onAction="#showParamRealtimeSetResultByFile"
                                                prefWidth="100.0" text="分文件查看">
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </padding>
                                        </Button>

                                        <Button layoutX="1700.0" layoutY="91"
                                                mnemonicParsing="false" onAction="#showSystemLog"
                                                prefWidth="100.0" text="查看系统日志">
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </padding>
                                        </Button>
                                    </children>
                                </AnchorPane>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
