                                        <Label layoutX="14.0" layoutY="137.0" prefHeight="15.0"
                                               text="vue页面目录(console-fund-ta-vue目录)"/>
                                        <TextField fx:id="vuePath" layoutX="300.0" layoutY="131.0" prefHeight="23.0"
                                                   prefWidth="770.0"/>

                                        <Label layoutX="14.0" layoutY="178.0" prefHeight="15.0"
                                               text="TA5字段对应关系文件"/>
                                        <TextField fx:id="ta5Path" layoutX="180.0" layoutY="171.0" prefHeight="23.0"
                                                   prefWidth="890.0"/>
                                        <Label layoutX="1100.0" layoutY="178.0" prefHeight="15.0" text="下个版本生效"/>

                                        <Label layoutX="14" layoutY="211.0" prefHeight="15.0" text="参数设置中可配置路径默认值" style="-fx-text-background-color: red;"/>

                                        <Button fx:id="executeRealtimeBtn" layoutX="1100.0" layoutY="91.0"
                                                mnemonicParsing="false" onAction="#executeRealtime" prefHeight="30.0"
                                                prefWidth="100.0" text="执行">
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </padding>
                                        </Button>
                                        <Label fx:id="errorTips" layoutX="1230" layoutY="96.0" prefHeight="15.0"
                                               style="-fx-font-weight: bold; -fx-text-background-color: red;"
                                               text="存在错误信息,请检查"/>
